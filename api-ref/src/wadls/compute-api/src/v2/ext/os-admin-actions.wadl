    <method name="POST" id="suspend">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN"
            title="Suspend server">
            <para role="shortdesc">Suspends a server. Changes its
                status to SUSPENDED.</para>
            <para>This operation stores the VM
                state on disk, writes all memory to disk, and stops
                the virtual machine.</para>
            <para>Administrative users might suspend an infrequently
                used instance or suspend an instance to perform system
                maintenance. Suspending an instance is similar to
                placing a device in hibernation; memory and vCPUs
                become available to create other instances.</para>
        </wadl:doc>
        <request>

            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook"
                    xml:lang="EN" title="Suspend server: JSON request">
                    <xsdxt:code
                        href="../api_samples/os-admin-actions/admin-actions-suspend.json"
                    />
                </wadl:doc>
            </representation>
            <representation mediaType="application/xml">
                <wadl:doc xmlns="http://docbook.org/ns/docbook"
                    xml:lang="EN" title="Suspend server: XML request">
                    <xsdxt:code
                        href="../api_samples/os-admin-actions/admin-actions-suspend.xml"
                    />
                </wadl:doc>
                <param name="suspend" style="plain" type="xsd:string"
                    required="true">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xml:lang="EN" title="Suspend action">
                        <para>Specify the <code>suspend</code> action
                            in the request body.</para>
                    </wadl:doc>
                </param>
            </representation>
        </request>
        <response status="202"/>
    </method>
